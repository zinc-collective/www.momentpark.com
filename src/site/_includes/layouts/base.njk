---
product_name: "Moment Park"
bodyClass: "leading-normal tracking-normal text-gray-900"
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/www.momentpark.com/css/main.css">
    <title>>{{ title }}</title>
  </head>
  <body {% if bodyClass %}class="{{ bodyClass }}"{% endif %}>

    <div class="container pt-24 md:pt-48 px-6 mx-auto flex flex-wrap flex-col md:flex-row items-center">
      <header>
        <h1 class="text-3xl">{{ title }}</h1>
        {%- if subtitle %}<p class="subtitle">{{ subtitle | safe }}</p>{% endif %}
      </header>
      {{ content | safe }}
      <footer class="w-full pt-16 pb-6 text-sm text-center md:text-left fade-in">
      <p><i class="cc cc-cc"></i><i class="cc cc-by"></i><i class="cc cc-sa"></i></p>
      <p>Except where otherwise noted, content on this site is licensed under a <a rel="license"
          href="http://creativecommons.org/licenses/by-sa/4.0/">Creative Commons Attribution-ShareAlike 4.0 International
          License</a>.</p>
      </footer>
    </div>
  </body>
</html>
